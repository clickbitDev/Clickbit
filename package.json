{
  "name": "clickbit-website",
  "version": "1.0.0",
  "description": "Custom web application for Clickbit.com.au - A modern, scalable platform with enhanced features and c$",
  "main": "server/index.js",
  "scripts": {
    "dev": "npm run kill-port && nodemon server/index.js",
    "server": "npm run kill-port && npm start",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "kill-port": "node server/scripts/kill-port.js",
    "kill-all-ports": "node server/scripts/kill-port.js --all",
    "clean-start": "npm run kill-all-ports && npm run dev",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "test:meta-tags": "node scripts/test-meta-tags.js",
    "test:meta-tags:url": "node scripts/test-meta-tags.js"
  },
  "keywords": [
    "clickbit",
    "web-app",
    "ecommerce",
    "custom-platform",
    "react",
    "nodejs"
  ],
  "author": "Clickbit.com.au",
  "license": "MIT",
  "dependencies": {
    "@paypal/paypal-server-sdk": "^1.1.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dompurify": "^3.2.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "express-winston": "^4.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.522.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.9.7",
    "quill": "^2.0.3",
    "react-datepicker": "^8.4.0",
    "react-parallax-tilt": "^1.7.298",
    "react-quill": "^2.0.0",
    "react-scroll": "^1.9.3",
    "sequelize": "^6.35.1",
    "sharp": "^0.32.6",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.7",
    "stripe": "^14.25.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/react-scroll": "^1.8.10",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "optionalDependencies": {
    "@google-cloud/bigquery": "^8.1.1"
  }
}
